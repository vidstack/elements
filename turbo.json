{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "analyze": {
      "dependsOn": ["^build:node", "^build:types"],
      "outputs": ["elements.json", "vscode.html-data.json", "react/**"]
    },
    "build": {
      "dependsOn": [
        "analyze",
        "^build:dev",
        "^build:prod",
        "^build:node",
        "^build:types",
        "^build:definitions"
      ]
    },
    "build:app": {
      "dependsOn": ["^analyze", "^build:node", "^build:prod", "^build:types", "^build:definitions"],
      "outputs": [".svelte-kit/**", "build/**"]
    },
    "build:dev": {
      "outputs": ["dist/**"]
    },
    "build:prod": {
      "outputs": ["dist-prod/**"]
    },
    "build:node": {
      "outputs": ["dist-node/**"]
    },
    "build:cdn": {
      "dependsOn": ["^build:prod"],
      "outputs": ["dist-cdn/**"]
    },
    "build:types": {
      "dependsOn": ["^build:types"],
      "outputs": ["types/**"]
    },
    "build:definitions": {
      "dependsOn": ["build:types"],
      "outputs": ["define/**"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["lint:eslint", "lint:prettier"]
    },
    "lint:eslint": {},
    "lint:prettier": {},
    "test": {}
  }
}
